<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

    <Button
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:id="@+id/delegation_btn"
            android:text="delegation"
            android:onClick="delegationClick"
            android:layout_width="match_parent"
            android:layout_height="50dp"/>
    <Button
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:id="@+id/callback_btn"
            android:text="callback"
            android:onClick="callbackClick"
            android:layout_width="match_parent"
            android:layout_height="50dp"/>

    <Button
            android:layout_marginTop="10dp"
            android:layout_marginBottom="10dp"
            android:id="@+id/builder"
            android:text="builder"
            android:onClick="builder"
            android:layout_width="match_parent"
            android:layout_height="50dp"/>


</LinearLayout>
